package com.mobilerechargeapk.model;

import java.util.Objects;

public class Admin {
    private String adminName;
    private String password;
	public String getName() {
		return adminName;
	}
	public void setName(String name) {
		this.adminName = name;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public Admin() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Admin(String name, String password) {
		super();
		this.adminName = name;
		this.password = password;
	}
	@Override
	public String toString() {
		return "Admin [adminName=" + adminName + ", password=" + password + "]";
	}
	@Override
	public int hashCode() {
		return Objects.hash(adminName, password);
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Admin other = (Admin) obj;
		return Objects.equals(adminName, other.adminName) && Objects.equals(password, other.password);
	}
    
    
}
